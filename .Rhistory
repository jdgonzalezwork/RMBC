plot(X,main="clusters estimation")
for (j in 1:3){
points(X[cluster==j,],pch=19, col=j+1)
}
points(X[ret$outliers,],pch=19,col=1)
### APLYING otrimle ALGORITHM
retot=otrimle::otrimle(data=X,G = 3)
plot(X,main="clusters estimation (otrimle)")
for (j in 1:3){
points(X[retot$cluster==j,],pch=19, col=j+1)
}
points(X[retot$cluster==0,],pch=19)
thetaOld.mu <- lapply(1:3, function(i){retot$mean[,i]})
thetaOld.sigma= lapply(1:3, function(i){retot$cov[,,i]})
thetaOld.alpha=retot$pi[2:4]
retInit=RMBCaux(Y=X,K=3, thetaOld.alpha,thetaOld.mu,
thetaOld.sigma,max_iter=80,
niterFixedPoint=1,tolerance=1e-3,cutoff=1-1e-3)
plot(X,main="clusters estimation(RMBC + otrimle)")
for (j in 1:3){
points(X[retInit$cluster==j,],pch=19, col=j+1)
}
points(X[retInit$outliers,],pch=19,col=1)
par(oldpar)
